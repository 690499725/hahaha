#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/叶片原型/项目1coze版本/project-demo1/node_modules/.store/vite@7.1.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/叶片原型/项目1coze版本/project-demo1/node_modules/.store/vite@7.1.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/vite@7.1.3/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.store/vite@7.1.3/node_modules/vite/bin/vite.js" "$@"
fi
